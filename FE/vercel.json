{
  "rewrites": [
    {
      "source": "\/api\/:path*",
      "destination": "https://iotfarm.onrender.com/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "\/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "\/(.*)",
      "dest": "/index.html"
    }
  ]
}
