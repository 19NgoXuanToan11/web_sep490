{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://iotfarm.onrender.com/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/payment-result",
      "dest": "/payment-result"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}

